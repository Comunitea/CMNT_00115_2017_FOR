<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- Add custom styles -->
    <template id="website_assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link href="/theme_forestal/static/css/_general.less" rel="stylesheet" type="text/less"/>
            <link href="/theme_forestal/static/css/header.less" rel="stylesheet" type="text/less"/>
            <link href="/theme_forestal/static/css/footer.less" rel="stylesheet" type="text/less"/>
            <link href="/theme_forestal/static/css/elements.less" rel="stylesheet" type="text/less"/>
        </xpath>

        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_forestal/static/js/main.js"/>
        </xpath>
    </template>

</odoo>